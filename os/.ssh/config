AddKeysToAgent  yes
Host republic
    HostName republic.stanford.edu

Host xenon.stanford.edu
    PubkeyAcceptedAlgorithms +ssh-rsa
    HostkeyAlgorithms +ssh-rsa

Host stanfordproxy
  HostName theory.stanford.edu
  User aozdemir
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
  HostkeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa

Host barrett*
  User aozdemir
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
  HostkeyAlgorithms +ssh-rsa
  PubkeyAcceptedKeyTypes +ssh-rsa

Host myth*.stanford.edu
  ForwardAgent yes
  PubkeyAcceptedAlgorithms +ssh-rsa
  HostkeyAlgorithms +ssh-rsa
